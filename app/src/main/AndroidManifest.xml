<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triple_c">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Triple-C"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TripleC">
        <activity
            android:name=".AddCar"
            android:exported="true" />
        <activity
            android:name=".Splash"
            android:exported="true" />
        <activity
            android:name=".ContactUs"
            android:exported="true" />
        <activity
            android:name=".OurServices"
            android:exported="true" />
        <activity
            android:name=".Details"
            android:exported="true" />
        <activity
            android:name=".AskForService"
            android:exported="true" />
        <activity
            android:name=".Profile"
            android:exported="true" />
        <activity
            android:name=".Confirm"
            android:exported="true"
            android:parentActivityName=".SignUp"/>
        <activity
            android:name=".SignUp"
            android:exported="true"
            android:parentActivityName=".SignIn"/>
        <activity
            android:name=".SignIn"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>